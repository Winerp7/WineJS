{
  "name": "winejs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www",
    "merge": "rimraf ./dist && tsc",
    "dev:clean": "rimraf ./dist",
    "dev:build": "tsc -w",
    "dev:run": "nodemon ./dist/bin/www",
    "dev": "concurrently npm:dev:*",
    "build": "tailwindcss build ./public/stylesheets/tailwind.css -o ./public/stylesheets/styles.css"
  },
  "dependencies": {
    "autoprefixer": "^10.0.1",
    "canvas": "^2.6.1",
    "chart.js": "^2.9.4",
    "chartjs-node-canvas": "^3.0.6",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "morgan": "~1.9.1",
    "node-chartist": "^1.0.5",
    "passport": "^0.4.1",
    "postcss-cli": "^8.1.0",
    "pug": "2.0.0-beta11",
    "slugify": "^1.4.5",
    "tailwindcss": "^1.9.6"
  },
  "devDependencies": {
    "@types/connect-flash": "0.0.35",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/http-errors": "^1.8.0",
    "@types/mongoose": "^5.7.37",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.6",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  }
}
